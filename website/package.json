{
  "name": "website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "yarn doxydoc && docusaurus build",
    "doxydoc": "/bin/bash -c 'cd .. && mkdir -p \"website/build/doxygen\" && doxygen vrs/Doxyfile'",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "ci": "yarn lint && yarn prettier:diff",
    "lint": "eslint --cache \"**/*.js\" && stylelint \"**/*.css\"",
    "format": "prettier --config .prettierrc --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "format:diff": "prettier --config .prettierrc --list-different \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "prettier:diff": "prettier --config .prettierrc --list-different \"**/*.{js,jsx,ts,tsx,md,mdx}\""
  },
  "dependencies": {
    "@docusaurus/core": "3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@mdx-js/react": "^3.1.0",
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^18.3.12",
    "clsx": "^2.1.1",
    "docusaurus-plugin-internaldocs-fb": "^1.19.2",
    "file-loader": "^6.2.0",
    "mermaid": "^11.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.31.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.4",
    "stylelint": "^16.18.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": "use yarn instead",
    "yarn": "^1.5"
  },
  "resolutions": {
    "@algolia/client-search": "^5.30.0",
    "@babel/core": "^7.26.12",
    "@docusaurus/theme-classic": "^3.8.1",
    "@mdx-js/react": "^3.1.0",
    "@types/react": "^18.3.12",
    "ansi-html": "0.0.8",
    "axios": "^1.8.3",
    "dompurify": "^3.2.4",
    "form-data": "^4.0.4",
    "micromatch": "^4.0.8",
    "nth-check": ">=2.0.1",
    "on-headers": "^1.1.0",
    "prism-react-renderer": "^2.4.0",
    "react-loadable": "^5.5.0",
    "semver": "^7.6.3",
    "shelljs": "^0.8.5",
    "typescript": "5.8.2",
    "webpack": "^5.95.0",
    "webpack-dev-server": "^5.2.2"
  }
}
